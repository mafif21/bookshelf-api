{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-console": "off",
    "quotes": [2, "double", { "avoidEscape": true }],
    "object-curly-newline": "off",
    "no-unused-vars": "off",
    "import/no-extraneous-dependencies": "off",
    "arrow-body-style": 0,
    "operator-linebreak": "off",
    "trailing-comma": false
  }
}
